{
  "name": "vovk-cli",
  "version": "0.0.1-beta.30",
  "bin": {
    "vovk": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "type": "module",
  "types": "./dist/index.d.mts",
  "prefix": ".",
  "scripts": {
    "build": "rm -rf dist tsconfig.build.tsbuildinfo && tsc -P tsconfig.build.json",
    "test": "npm run build && chmod +x ./dist/index.mjs && NODE_OPTIONS=\"--loader ts-node/esm\" TS_NODE_PROJECT=./tsconfig.test.json node --test test/**.mts",
    "ncu": "npm-check-updates -u",
    "npm-publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finom/vovk.git"
  },
  "keywords": [
    "nextjs",
    "router",
    "cli",
    "vovk"
  ],
  "author": "Andrii Gubanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finom/vovk/issues"
  },
  "homepage": "https://vovk.dev",
  "peerDependencies": {
    "vovk": "^3.0.0-beta.32"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.5.0",
    "@npmcli/package-json": "^5.2.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "concurrently": "^8.2.2",
    "ejs": "^3.1.10",
    "gray-matter": "^4.0.3",
    "inflection": "^3.0.0",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "pluralize": "^8.0.0",
    "prettier": "^3.3.3",
    "tar-stream": "^3.1.7",
    "ts-morph": "^23.0.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/npmcli__package-json": "^4.0.4",
    "@types/pluralize": "^0.0.33",
    "@types/tar-stream": "^3.1.3",
    "create-next-app": "^14.2.13",
    "type-fest": "^4.26.0"
  }
}
