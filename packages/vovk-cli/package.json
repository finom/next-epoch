  },
  "dependencies": {
    "@inquirer/prompts": "^5.5.0",
    "@npmcli/package-json": "^5.2.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "concurrently": "^8.2.2",
    "inflection": "^3.0.0",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
