{
  "name": "vovk-cli",
  "version": "0.0.1-beta.17",
  "bin": {
    "vovk": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "type": "module",
  "types": "./dist/index.d.mts",
  "prefix": ".",
  "scripts": {
    "build": "rm -rf dist tsconfig.build.tsbuildinfo && tsc -P tsconfig.build.json",
