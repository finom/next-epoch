{
  "ClientController": {
    "controllerName": "ClientController",
    "_prefix": "client",
    "_handlers": {
      "getHelloWorld": {
        "path": "client-controller/get-hello-world",
        "httpMethod": "GET"
      },
      "getWithParams": {
        "path": "with-params/:hello",
        "httpMethod": "GET"
      },
      "postWithParams": {
        "path": "with-params/:hello",
        "httpMethod": "POST"
      },
      "postWithEqualityValidation": {
        "clientValidators": {
          "body": {
            "hello": "body"
          },
          "query": {
            "hey": "query"
          }
        },
        "path": "client-controller/post-with-equality-validation",
        "httpMethod": "POST"
      },
      "postWithZodValidation": {
        "clientValidators": {
          "body": {
            "type": "object",
            "properties": {
              "hello": {
                "type": "string"
              }
            },
            "required": [
              "hello"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
          }
        },
        "path": "client-controller/post-with-zod-validation",
        "httpMethod": "POST"
      }
    }
  }
}